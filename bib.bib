	Number = {3},
	Pages = {287--295},
	Publisher = {Springer},
	Title = {{Beyond two-cell networks: experimental measurement of neuronal responses to multiple synaptic inputs.}},
	Url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed\&id=15830165\&retmode=ref\&cmd=prlinks papers2://publication/doi/10.1007/s10827-005-0336-9},
	Volume = {18},
